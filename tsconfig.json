{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": false,
        "baseUrl": "./",
        "paths": {
            "@shared/*": ["./src/shared/*"],
            "@features/*": ["./src/features/*"],
            "@core/*": ["./src/core/*"],
            "@routes": ["./src/routes"],
            "@assets/*": ["./src/assets/*"],
            "@hooks/*": ["./src/hooks/*"],
            "@store/*": ["./src/store/*"]
        },
        "typeRoots": ["./src/types"],
        "types": ["...", "@types/jest"]
    }
}
